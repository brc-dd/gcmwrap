{
  "name": "gcmwrap",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "fmt": {
    "lineWidth": 100,
    "proseWrap": "never",
    "semiColons": false,
    "singleQuote": true
  },
  "compilerOptions": {
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "noUncheckedIndexedAccess": true
  }
}
